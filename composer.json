{
    "name": "atomickitten/framework",
    "type": "typo3-flow-package",
    "description": "Atomic Design framework implemented using Flow and Fluid.",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Daniel Siepmann",
            "email": "coding@daniel-siepmann.de",
            "homepage": "http://daniel-siepmann.de",
            "role": "Developer"
        }
    ],
    "support": {
        "docs": "http://atomickittenframework.readthedocs.org/"
    },
    "config": {
        "vendor-dir": "Packages/Libraries",
        "bin-dir": "bin"
    },
    "require": {
        "typo3/flow": "~3.1.0",
        "atomickitten/example": "*"
    },
    "autoload": {
        "psr-0": {
            "AtomicKitten\\Framework": "Classes"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "post-update-cmd": "TYPO3\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-install-cmd": "TYPO3\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-package-update": "TYPO3\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall",
        "post-package-install": "TYPO3\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall"
    }
}
